! Vim Powerline Fonts
URxvt*font: xft:Hack:size=11
! Window title: nintF1link
URxvt*title:nintF1link
! Icon
! URxvt*iconFile:/usr/share/icons/Numix-Circle/48/apps/0ad.svg
! Hide scrollbars
URxvt*scrollBar:false
